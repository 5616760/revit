<Window x:Class="Tunnel.ShowProgressBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tunnel"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="450">
    <Grid>
        <StackPanel>
            <Label Margin="15 5">模型生成进度：</Label>
            <ProgressBar Margin="15 15" Height="15" x:Name="ProgressBar1"></ProgressBar>
        </StackPanel>
    </Grid>
</Window>
